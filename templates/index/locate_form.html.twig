<div class="locate-form-container">
    <div class="locate-form">
        <form id="locate-entity-form" method="POST" data-url="{{ dataUrl }}" data-method="POST">
            {% if errors is defined %}
                {% for error in errors %}
                    <span class="error">{{ error | trans }}</span><br/>
                {%  endfor %}
            {% endif %}
            <label for="locate-form-input">Id:</label>
            <input id="locate-form-input" type="text" class="locate-form-input" name="id">
            <div class="locate-form-submit-container">
                <button type="submit" class="btn btn-primary">{{ 'Locate' | trans }}</button>
            </div>
        </form>
    </div>
</div>
